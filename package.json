{
  "name": "template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "devStart": "nodemon ./src/App.ts",
    "start": "npm run clean && npm run build && node ./build/App.js",
    "build": "tsc",
    "clean": "rm -rf build",
    "test": "npm run test:unit && npm run test:aceptance",
    "test:unit": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.ts'",
    "test:aceptance": "node -r esm ./node_modules/.bin/cucumber-js",
	"test:coverage": "nyc --nycrc-path config/nycrc-config.json npm run test",
	"migration:create": "npx dotenv -- typeorm migration:create -n",
	"migration:run": "rm -f ./migration/*.js && tsc migration/*.ts && npx dotenv -- ts-node ./node_modules/typeorm/cli.js migration:run",
	"migration:revert": "rm -f ./migration/*.js && tsc migration/*.ts && npx dotenv -- ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "author": "seblaz mpicco FrancoMaz franfusaro",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "log4js": "^6.3.0",
    "nyc": "^15.1.0",
    "pg": "^8.4.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.0.0-rc.0",
    "@types/chai": "^4.2.13",
    "@types/express": "^4.17.8",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.8",
    "chai": "^4.2.0",
    "codecov": "^3.8.0",
    "dotenv-cli": "^4.0.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "sinon": "^9.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
